{
  "name": "cson-cli",
  "version": "1.0.0",
  "keywords": [
    "cson",
    "json",
    "parser",
    "formatter",
    "conversion"
  ],
  "description": "cson2json and json2cson convenience shell tools",
  "bin": {
    "cson2json": "bin/cson2json",
    "json2cson": "bin/json2cson"
  },
  "scripts": {
    "build": "npub prep && coffee -cbo lib src",
    "prepublish": "rm -rf lib && npm run build",
    "pretest": "npm run build",
    "test": "test/check"
  },
  "author": {
    "name": "Johan Sundstr√∂m",
    "email": "oyasumi@gmail.com"
  },
  "dependencies": {
    "cson-safe": "^1.0.2",
    "format-json": "^1.0.3"
  },
  "devDependencies": {
    "assertive": "^1.4.1",
    "coffee-script": "^1.8.0",
    "npub": "~0.5.1"
  },
  "publishConfig": {
    "license": {
      "exclude": [
        "lib",
        "src",
        "test"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/johan/cson-cli.git"
  }
}
